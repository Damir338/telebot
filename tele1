import random 

import telebot 



bot = telebot.TeleBot('1624666143:AAF87yylBAFxSDA7rbEdc2s7vnajfsNwEO4')



from telebot import types



 

# Вопросы



#IT1 = ["И так певый вопрос!\nВ каком году вышел первый язык програмирования?"]





# Метод, который получает сообщения и обрабатывает их



@bot.message_handler(content_types=['text'])



def get_text_messages(message):

    



    if message.text == "Привет" or message.text == "/start" or message.text == "/начать":



        # Готовим кнопки



        keyboard = types.InlineKeyboardMarkup()



        key_1 = types.InlineKeyboardButton(text='Будущие первоклассники', callback_data='choice1')



        keyboard.add(key_1)



        key_2 = types.InlineKeyboardButton(text='Дополнительное образование', callback_data='choice2')



        keyboard.add(key_2)



        key_3 = types.InlineKeyboardButton(text='Дошкольные группы', callback_data='choice3')



        keyboard.add(key_3)



        key_4 = types.InlineKeyboardButton(text='Зачисление ребенка', callback_data='choice4')



        keyboard.add(key_4)



        key_5 = types.InlineKeyboardButton(text='Направления обучения', callback_data='choice5')



        keyboard.add(key_5)



        key_6 = types.InlineKeyboardButton(text='Новостройки', callback_data='choice6')



        keyboard.add(key_6)



        key_7 = types.InlineKeyboardButton(text='Организация безопасности', callback_data='choice7')



        keyboard.add(key_7)



        key_8 = types.InlineKeyboardButton(text='Организация обучения', callback_data='choice8')



        keyboard.add(key_8)



        key_9 = types.InlineKeyboardButton(text='Питание', callback_data='choice9')



        keyboard.add(key_9)



        key_10 = types.InlineKeyboardButton(text='ЭЖД', callback_data='choice10')



        keyboard.add(key_10)                               



        # Показываем все кнопки сразу и пишем сообщение о выборе



        bot.send_message(message.from_user.id, text='Выбирите тему вопроса:', reply_markup=keyboard)



    elif message.text == "/help":



        bot.send_message(message.from_user.id, "Напишите Привет или /start")



    else:



        bot.send_message(message.from_user.id, "Я вас не понимаю. Напишите /help.")



 

# Обработчик нажатий на кнопки



